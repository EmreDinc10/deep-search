# Copy environment variables from main .env file
AZURE_OPENAI_API_KEY=
AZURE_OPENAI_ENDPOINT=
AZURE_OPENAI_DEPLOYMENT_NAME=
AZURE_OPENAI_API_VERSION=

# Google Custom Search API Configuration (optional for Google search)
# Get API key: https://developers.google.com/custom-search/v1/introduction
# Create CSE: https://cse.google.com/cse/create/new
GOOGLE_API_KEY=
GOOGLE_CSE_ID=

# SerpAPI Configuration (alternative for Google search)
# Get API key: https://serpapi.com/
SERPAPI_KEY=

# Optional Reddit API credentials
REDDIT_CLIENT_ID=
REDDIT_CLIENT_SECRET=
REDDIT_USER_AGENT=

# Server configuration
PORT=8000
HOST=0.0.0.0
